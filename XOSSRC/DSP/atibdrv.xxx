	DATA

lit23:	.LONG	23t
lit24:	.LONG	24t

nameun:	.ASCIZ	"Mach64-??"
nameGX:	.ASCIZ	"Mach64-GX"
nameCX:	.ASCIZ	"Mach64-CX"
nameCT:	.ASCIZ	"Mach64-CT"
nameET:	.ASCIZ	"Mach64-ET"
nameVT:	.ASCIZ	"Mach64-VT"
