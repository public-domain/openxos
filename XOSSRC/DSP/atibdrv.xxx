;Define macros used to generate the initialization tables

	.MACRO	TABLE	n
	.NLIST
	.MOD	4

	.MACRO	T  a1,a2,a3,a4,a5,a6
atitable''a'n::
	.ENDM

	.MACRO	L  lbl
.IF DEF lbl
  .IF NE lbl-{$-atitable'n}
	.IRP	p, >lbl
	.IRP	q, >{$-atitable'n}
	.ERROR	Incorrect label offset: lbl =  p, should be q
	.ENDR
	.ENDR
  .ENDC
.IFF
  lbl=!$-atitable'n
